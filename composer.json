{
    "name": "virhi/lazy-mock-api-bundle",
    "description": "generate a Rest Hal APi on top of doctrine orm",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Rhino",
            "email": "vincent.rhino@gmail.com",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "Virhi\\LazyMockApiBundle\\": "src/Virhi/LazyMockApiBundle/"
        }
    },

    "autoload-dev": {
        "psr-4": { "Virhi\\LazyMockApiBundle\\Tests\\": "Tests/" }
    },

    "require": {
        "symfony/symfony": "~2.3",
        "snc/redis-bundle": "1.1.*",
        "predis/predis": "0.8.*",
        "virhi/admin-bundle": "0.1.*",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "virhi/component": "0.0.*"
    },

    "require-dev": {
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "2.5.x-dev",
        "behat/symfony2-extension": "1.1.*@dev",
        "behat/mink-extension": "1.3.*@dev",
        "behat/mink-browserkit-driver": "1.1.*@dev",
        "behat/mink-goutte-driver": "~1.0",
        "behat/gherkin": "2.3.*@dev",
        "behat/common-contexts" : "1.2.*",
        "khepin/yaml-fixtures-bundle": "~0.8.1",
        "misd/guzzle-bundle": "~1.0"
    }
}
